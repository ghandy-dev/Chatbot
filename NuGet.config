<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="github" value="https://nuget.pkg.github.com/ghandy-dev/index.json" />
        <add key="localFallback" value="./libs" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="%GITHUB_USERNAME%" />
            <add key="ClearTextPassword" value="%GITHUB_PACKAGES_TOKEN%" />
        </github>
    </packageSourceCredentials>
</configuration>
